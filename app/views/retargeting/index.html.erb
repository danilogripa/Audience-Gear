<% if @affiliates.count > 0 %>
  <pre style="word-wrap: break-word; white-space: pre-wrap;">
    <% @affiliates.each do |affiliate| %>
        <%= "// #{affiliate.name}\n"%>
        <%= raw affiliate.partner %>
        <%= "\n// END #{affiliate.name}\n"%>
    <% end %>
  </pre>
<% else %>
  // Campanha desabilitada
<% end %>
